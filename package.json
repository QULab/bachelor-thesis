{
  "name": "bachelor-thesis",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "docker-build": "docker build -t bachelor-website:dev website",
    "start": "cd frontend && npm run start",
    "build": "cd frontend && npm run build",
    "test": "jest --coverage",
    "lint": "eslint '{website,data-processing}/**/*.js'",
    "prettier": "prettier --write '**/*.{js,json}'",
    "process-texts": "source venv/bin/activate && python website/process_texts.py",
    "postinstall": "cd website/frontend && npm install && cd ../backend && npm install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "@testing-library/user-event": "^10.0.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.samplesize": "^4.2.0",
    "node-html-parser": "^1.2.14",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "xlsx": "^0.15.6"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "babel-jest": "^26.3.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead"
  ],
  "engines": {
    "node": ">=12.13.0 <13"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ]
  }
}
